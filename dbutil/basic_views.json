{
    "_id": "_design/basic_views",
    "views": {
        "all_docs": {
            "map": "function(doc) { emit(null, doc); }"
        },
        "count_docs": {
            "map": "function(doc) { emit(null, doc); }",
            "reduce": "_count(keys, vals, rereduce)"
        }
    }
}